		<script id="result_registrar" type="text/x-handlebars-template">
			<h6><strong>Результат</strong></h6>
			{{#if result}}
				<p>Номер операции - {{clTRID}}</p>
				{{#each result}}
					<p>{{this.code}}: {{this.msg}} {{#if this.reason}}, {{this.reason}} {{/if}}</p>
				{{/each}}
			{{/if}}
			{{#if message}}
				{{#if error}}
					{{message}}
				{{else}}
					<p>Есть неразобранные сообщения реестра</p>
				{{/if}}
			{{/if}}
			{{#if registrar}}
				<h6><strong>Результат</strong></h6>
				<p>id: {{registrar.id}}</p>
				{{#if registrar.status}}
					<p>status: {{#each registrar.status}}{{this}} {{/each}}</p>
				{{/if}}
				<p>Org: {{registrar.locPostalInfo.org}}</p>
				<p>Address: {{#each registrar.locPostalInfo.address}}{{this}} {{/each}}</p>
				<p>Int Org: {{registrar.intPostalInfo.org}}</p>
				<p>Legal Address: {{#each registrar.legalInfo.address}}{{this}} {{/each}}</p>
				<p>voice: {{#each registrar.voice}}{{this}} {{/each}}</p>
				{{#if registrar.fax}}
					<p>status: {{#each registrar.fax}}{{this}} {{/each}}</p>
				{{/if}}
				<p>taxpayerNumbers: {{registrar.taxpayerNumbers}}</p>
				{{#if registrar.email}}
					{{#each registrar.email}}
						<p>{{@key}}: {{this}}</p>
					{{/each}}
				{{/if}}
				<p>www: {{registrar.www}}</p>
				<p>whois: {{registrar.whois}}</p>
				{{#if registrar.v4}}
					<p>v4: {{#each registrar.v4}}{{this}} {{/each}}</p>
				{{/if}}
				{{#if registrar.v6}}
					<p>v6: {{#each registrar.v6}}{{this}} {{/each}}</p>
				{{/if}}
				{{#if registrar.upDate}}
					<p>upDate: {{registrar.upDate}}</p>
				{{/if}}
				{{#if registrar.crDate}}
					<p>upDate: {{registrar.crDate}}</p>
				{{/if}}
			{{/if}}
			{{#if limits}}
				{{#each limits}}
					<h6><strong>{{@key}}</strong></h6>
					{{#each this}}
						<p>{{@key}}: {{this}}</p>
					{{/each}}
				{{/each}}
			{{/if}}
			{{#if balance}}
				<h6><strong>Баланс регистратора</strong></h6>
				{{#each balance}}
					<p>{{@key}}: {{this}}</p>
				{{/each}}
			{{/if}}
			{{#if billing}}
				<h6><strong>Стоимость услуг, оказанных регистратору с начала расчетного периода</strong></h6>
				{{#each billing}}
					<p>{{@key}}: {{this}}</p>
				{{/each}}
			{{/if}}
			{{#if forecast}}
				<h6><strong>Прогноз платежей на расчетный период</strong></h6>
				{{#each forecast}}
					<p>{{@key}}: {{this}}</p>
				{{/each}}
			{{/if}}
			{{#if poll}}
				{{#each poll}}
					<hr/>
					{{#each this}}
						<p>{{@key}}: {{this}}</p>
					{{/each}}
				{{/each}}
			{{/if}}
		</script>

