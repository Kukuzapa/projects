		<script id="history_templ" type="text/x-handlebars-template">
			<table class="table table-striped">
				<thead>
					<tr>
						<th scope="col">trID</th>
						<th scope="col">object</th>
						<th scope="col">command</th>
						<th scope="col">req</th>
						<th scope="col">res</th>
						<th scope="col">b_date</th>
						<th scope="col">e_date</th>
						<th scope="col">status</th>
					</tr>
				</thead>
				<tbody>
					{{#each .}}
						<tr>
							<td id="{{this.id}}" class="trid">{{this.id}}</td>
							<td>{{this.object}}</td>
							<td>{{this.command}}</td>
							<td data-toggle="popover" title="Request" data-content='{{this.request}}'>Развернуть запрос</td>
							<td data-toggle="popover" title="Response" data-content='{{this.response}}'>Показать ответ</td>
							<td>{{this.date}}</td>
							<td>{{this.date_cron}}</td>
							<td>{{this.status}}</td>
							
						</tr>
					{{/each}}
				</tbody>
			</table>
		</script>

		<script id="domainTable_templ" type="text/x-handlebars-template">
			{{#each .}}
			<tr>
				<td onclick="go_to_update($(this).text())">{{this.name}}</td>
				<td onclick="go_to_contact($(this).text())">{{this.contact}}</td>
				<td class="poptest">
					{{#each this.status}}
						{{this}}</br>
					{{/each}}
				</td>
				<td>
					{{#each this.nss}}
						{{this}}</br>
					{{/each}}
				</td>
				<td data-domain="{{this.name}}" onclick="go_to_renew($(this).data('domain'))">{{this.exdate}}</td>
				<!--td>{{this.upddate}}</td-->
			</tr>
			{{/each}}
		</script>

		<script id="add_nss" type="text/x-handlebars-template">
			<div class="row full-nss-{{count}}">
				<div class="input-group mb-3 auto-nss col-6">
					<input type="text" value="{{ns}}" class="form-control form-control-sm dom_{{command}}_nss_val" data-ip="ip-{{count}}">
				</div>
				<div class="input-group mb-3 auto-nss col-6">
					<input type="text" value="{{ip}}" class="form-control form-control-sm" id="ip-{{count}}">
					<div class="input-group-append" data-full="full-nss-{{count}}" onclick = "rem_sts_nss(this)">
						<span class="input-group-text">-</span>
					</div>
				</div>
			</div>
		</script>

		<script id="add_sts" type="text/x-handlebars-template">
			<div class="input-group mb-3 auto-sts-{{obj}} col-6 full-sts-{{count}}">
